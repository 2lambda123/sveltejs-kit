{
	"root": true,
	"extends": "@sveltejs",
	"parserOptions": {
		"sourceType": "module"
	},
	"globals": {
		"goto": true,
		"invalidate": true,
		"prefetch": true,
		"prefetchRoutes": true,
		"beforeNavigate": true,
		"afterNavigate": true
	},
	"rules": {
		"@typescript-eslint/no-empty-interface": "off",
		"@typescript-eslint/method-signature-style": ["error", "method"]
	}
}
