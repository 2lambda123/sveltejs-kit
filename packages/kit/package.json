{
	"name": "@sveltejs/kit",
	"version": "1.0.0-next.34",
	"type": "module",
	"dependencies": {
		"@sveltejs/vite-plugin-svelte": "workspace:*",
		"cheap-watch": "^1.0.3",
		"http-proxy": "^1.18.1",
		"sade": "^1.7.4",
		"scorta": "^1.0.0",
		"source-map": "^0.7.3"
	},
	"devDependencies": {
		"@rollup/plugin-typescript": "^8.2.0",
		"@sveltejs/app-utils": "workspace:*",
		"@types/node": "^14.14.22",
		"@types/rimraf": "^3.0.0",
		"@types/sade": "^1.7.2",
		"amphtml-validator": "^1.0.34",
		"eslint": "^7.19.0",
		"estree-walker": "^2.0.2",
		"is-reference": "^1.2.1",
		"kleur": "^4.1.4",
		"magic-string": "^0.25.7",
		"meriyah": "^4.1.2",
		"node-fetch": "^2.6.1",
		"periscopic": "^3.0.0",
		"port-authority": "^1.1.2",
		"require-relative": "^0.8.7",
		"rimraf": "^3.0.2",
		"rollup": "^2.38.3",
		"sirv": "^1.0.11",
		"tiny-glob": "^0.2.8",
		"typescript": "^4.2.2",
		"vite": "^2.0.4"
	},
	"peerDependencies": {
		"svelte": "^3.32.1",
		"vite": "^2.0.4"
	},
	"bin": {
		"svelte-kit": "svelte-kit.js"
	},
	"files": [
		"assets",
		"dist",
		"client"
	],
	"scripts": {
		"dev": "rollup -cw",
		"build": "rollup -c",
		"lint": "eslint --ignore-path .gitignore \"src/**/*.{ts,mjs,js,svelte}\" && npm run check-format",
		"format": "prettier --write . --config ../../.prettierrc --ignore-path .gitignore",
		"check-format": "prettier --check . --config ../../.prettierrc --ignore-path .gitignore",
		"prepublishOnly": "npm run build",
		"test": "uvu src \"(spec\\.mjs|test[\\\\/]index\\.mjs)\""
	},
	"exports": {
		"./api": {
			"import": "./dist/api.js"
		},
		"./renderer": {
			"import": "./dist/renderer.js"
		}
	}
}
