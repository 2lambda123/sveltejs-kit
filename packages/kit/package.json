{
	"name": "@sveltejs/kit",
	"version": "1.0.0-next.29",
	"type": "module",
	"dependencies": {
		"cheap-watch": "^1.0.2",
		"http-proxy": "^1.18.1",
		"rollup": "^2.32.0",
		"rollup-plugin-css-chunks": "^2.0.2",
		"rollup-plugin-terser": "^7.0.2",
		"sade": "^1.7.4",
		"scorta": "^1.0.0",
		"snowpack": "^3.0.10",
		"source-map": "^0.7.3"
	},
	"devDependencies": {
		"@sveltejs/app-utils": "*",
		"@types/node": "^14.11.10",
		"@types/rimraf": "^3.0.0",
		"@types/sade": "^1.7.2",
		"amphtml-validator": "^1.0.34",
		"eslint": "^7.14.0",
		"estree-walker": "^3.0.0",
		"is-reference": "^1.2.1",
		"kleur": "^4.1.3",
		"magic-string": "^0.25.7",
		"meriyah": "^3.0.3",
		"node-fetch": "^2.6.1",
		"periscopic": "^3.0.0",
		"port-authority": "^1.1.1",
		"require-relative": "^0.8.7",
		"rimraf": "^3.0.2",
		"sirv": "^1.0.7",
		"source-map-support": "^0.5.19",
		"svelte": "^3.29.0",
		"tiny-glob": "^0.2.8",
		"uvu": "^0.5.1"
	},
	"bin": {
		"svelte-kit": "svelte-kit"
	},
	"files": [
		"assets",
		"dist",
		"client"
	],
	"scripts": {
		"dev": "rollup -cw",
		"build": "rollup -c",
		"lint": "eslint --ignore-path .gitignore \"**/*.{ts,mjs,js,svelte}\" && npm run check-format",
		"format": "prettier --write . --config ../../.prettierrc --ignore-path .gitignore",
		"check-format": "prettier --check . --config ../../.prettierrc --ignore-path .gitignore",
		"prepublishOnly": "npm run build",
		"test": "uvu src \"(spec.mjs|test/index.mjs)\""
	}
}
