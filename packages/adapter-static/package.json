{
	"name": "@sveltejs/adapter-static",
	"version": "0.0.14",
	"scripts": {
		"build": "rollup -c",
		"lint": "eslint --ignore-pattern node_modules/ \"**/*.{ts,js,svelte}\" && npm run check-format",
		"format": "prettier --write . --config .gitignore --ignore-path ../../.prettierignore",
		"check-format": "prettier --check . --config .gitignore --ignore-path ../../.prettierignore"
	},
	"dependencies": {
		"@sveltejs/app-utils": "workspace:*"
	}
}
